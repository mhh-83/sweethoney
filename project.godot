; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SweetHoney"
run/main_scene="res://scenes/intro.tscn"
config/auto_accept_quit=false
config/quit_on_go_back=false
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/image="res://sprite/icon.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://sprite/icon.png"
boot_splash/show_image.mobile=false
boot_splash/show_image.android=false
config/auto_accept_quit.windows=true

[audio]

general/text_to_speech=true
driver/enable_input=true

[autoload]

AddBee="*res://script/add_bee.gd"
Exit="*res://script/exit.gd"
GlobalTime="*res://script/time.gd"
CheckInternet="*res://script/check_internet.gd"

[debug]

file_logging/enable_file_logging=true
gdscript/warnings/enable=false
gdscript/warnings/inference_on_variant=0
gdscript/warnings/native_method_override=0
gdscript/warnings/get_node_default_without_onready=0
gdscript/warnings/onready_with_export=0

[display]

window/size/viewport_width=1000
window/size/viewport_height=2000
window/size/mode=2
window/size/transparent=true
window/stretch/mode="canvas_items"
window/per_pixel_transparency/allowed=true
window/handheld/orientation=1
window/ios/hide_home_indicator=false
window/ios/hide_status_bar=false

[editor_plugins]

enabled=PackedStringArray("res://addons/HTTPManager/plugin.cfg", "res://addons/wiggly_appendage_2d/plugin.cfg")

[filesystem]

import/blender/enabled.android=true
import/fbx/enabled.android=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[internationalization]

locale/include_text_server_data=true
locale/fallback="fa_IR"

[physics]

2d/physics_engine="GodotPhysics2D"

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
vrs/mode=1
vrs/texture="res://icon.png"
